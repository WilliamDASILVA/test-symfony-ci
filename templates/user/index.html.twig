{% extends 'base.html.twig' %}

{% block title %}Users{% endblock %}

{% block body %}

    {{ form_start(form) }}

    {{ form_widget(form) }}

    {{ form_end(form) }}


    {% if users|length %}

        <ul>
            {% for user in users %}
                <li>
                    {{ user.id }} {{ user.email }}
                </li>
            {% endfor %}
        </ul>

    {% else %}

        <p>no users</p>

    {% endif %}

{% endblock %}
